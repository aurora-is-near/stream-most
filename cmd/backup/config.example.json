{
  "MessagesFormat": "near_v3",
  "Input": {
    "Nats": {
      "Endpoints": [
        "nats://localhost:4222/"
      ],
      "TimeoutMs": 10000,
      "PingIntervalMs": 600000,
      "MaxPingsOutstanding": 5,
      "LogTag": "input"
    },
    "Stream": "myblocks",
    "Subject": "",
    "RequestWaitMs": 5000
  },
  "Reader": {
    "MaxRps": 2,
    "BufferSize": 1000,
    "MaxRequestBatchSize": 250,
    "SubscribeAckWaitMs": 5000,
    "InactiveThresholdSeconds": 300,
    "FetchTimeoutMs": 10000,
    "SortBatch": true,
    "LastSeqUpdateIntervalSeconds": 5,
    "Durable": "myconsumer",
    "StrictStart": false,
    "WrongSeqToleranceWindow": 50
  },
  "FromSeq": 0,
  "ToSeq": 1000,
  "OutputDir": "out/backup",
  "ChunkPrefix": "chunk_",
  "CompressionLevel": 6,
  "MaxChunkEntries": 1000000,
  "MaxChunkSize": 50,
  "Monitoring": {
    "ListenAddress": "localhost:9991",
    "Namespace": "infra",
    "Subsystem": "stream_bridge",
    "StdoutIntervalSeconds": 10
  }
}
